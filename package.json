{
  "name": "taro-weapp-template",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "vue3-NutUI4",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev:weapp": "taro build --type weapp --mode development --watch",
    "dev:weapp[node-prod]": "taro build --type weapp --mode development --env production --watch",
    "build:weapp--staging": "taro build --type weapp --mode staging",
    "build:weapp--prod": "taro build --type weapp --mode production",
    "dev:h5": "taro build --type h5 --mode development --watch",
    "build:h5--staging": "taro build --type h5 --mode staging",
    "build:h5--prod": "taro build --type h5 --mode production",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .vue,.ts,.tsx .",
    "lint:js--fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint \"src/**/*.{vue,css,scss,postcss}\"",
    "lint:style--fix": "npm run lint:style -- --fix",
    "type:check": "tsc --noEmit --skipLibCheck",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@nutui/icons-vue-taro": "^0.0.9",
    "@nutui/nutui-taro": "^4.0.4",
    "@tarojs/components": "3.6.20",
    "@tarojs/helper": "3.6.20",
    "@tarojs/plugin-framework-vue3": "3.6.20",
    "@tarojs/plugin-html": "3.6.20",
    "@tarojs/plugin-platform-alipay": "3.6.20",
    "@tarojs/plugin-platform-h5": "3.6.20",
    "@tarojs/plugin-platform-jd": "3.6.20",
    "@tarojs/plugin-platform-qq": "3.6.20",
    "@tarojs/plugin-platform-swan": "3.6.20",
    "@tarojs/plugin-platform-tt": "3.6.20",
    "@tarojs/plugin-platform-weapp": "3.6.20",
    "@tarojs/runtime": "3.6.20",
    "@tarojs/shared": "3.6.20",
    "@tarojs/taro": "3.6.20",
    "better-mock": "^0.3.7",
    "dayjs": "^1.11.10",
    "pinia": "^2.1.7",
    "taro-plugin-pinia": "^1.0.0",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/cli": "3.6.20",
    "@tarojs/taro-loader": "3.6.20",
    "@tarojs/webpack5-runner": "3.6.20",
    "@types/node": "^18.15.11",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.40",
    "babel-preset-taro": "3.6.20",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-taro": "3.6.20",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.2",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "unplugin-vue-components": "^0.23.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-loader": "^17.0.0",
    "webpack": "^5.78.0"
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": "eslint",
    "*.{vue,css,less,scss,postcss}": "stylelint"
  }
}
